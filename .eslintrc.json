{
  "root": true,
  "parser": "vue-eslint-parser",
  "parserOptions": {
    "parser": "@typescript-eslint/parser",
    "sourceType": "module"
  },
  "plugins": [
    "@typescript-eslint",
    "vue"
  ],
  "extends": [
    "plugin:vue/vue3-strongly-recommended",
    "prettier",
    "prettier/@typescript-eslint",
    "prettier/vue"
  ],
  "rules": {
    "vue/html-indent": [
      "error",
      "tab"
    ],
    "no-console": 0,
    "no-warning-comments": 1
  }
}
